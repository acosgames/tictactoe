{
    "name": "tictactoe",
    "sideEffects": [
        "*.css"
    ],
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "start": "npm run vite:dev & npm run server:dev2 & npx acos dev --type=vite",
        "vite:build": "vite build",
        "vite:dev": "vite serve",
        "client:dev": "npx webpack --config builds/webpack/client/dev.config.js --watch",
        "client:prod": "npx webpack --config builds/webpack/client/prod.config.js",
        "server:dev2": "npx rollup -c -w",
        "server:dev": "npx webpack --config builds/webpack/server/dev.config.js --watch ./game-server",
        "server:prod": "npx webpack --config builds/webpack/server/prod.config.js",
        "deploy": "npm run client:prod && npm run server:prod && npx acos deploy",
        "kill:win": "taskkill //im node.exe //F"
    },
    "dependencies": {
        "acosgames": "file:../acosgames",
        "react": "^18.3.1",
        "react-bucketjs": "^1.0.2",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@vitejs/plugin-react": "^4.2.1",
        "sass": "^1.63.6",
        "vite": "^5.2.11",
        "webpack": "^5.88.1",
        "webpack-cli": "^5.1.4"
    }
}
