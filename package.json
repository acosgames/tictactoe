{
    "name": "tictactoe",
    "sideEffects": [
        "*.css"
    ],
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "start": "npm run client:dev & npm run server:dev & npx acos dev --type=vite",
        "client:dev": "vite serve",
        "server:dev": "npx rollup -c -w",
        "client:build": "vite build",
        "server:build": "npx rollup -c --environment NODE_ENV:production",
        "build": "npm run client:build && npm run server:build",
        "deploy": "npm run client:build && npm run server:build && npx acos deploy",
        "kill:win": "taskkill //im node.exe //F"
    },
    "dependencies": {
        "acosgames": "file:../acosgames",
        "react": "^18.3.1",
        "react-bucketjs": "^1.0.2",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@vitejs/plugin-react": "^4.2.1",
        "sass": "^1.63.6",
        "vite": "^5.2.11",
        "vite-plugin-css-injected-by-js": "^3.5.1",
        "webpack": "^5.88.1",
        "webpack-cli": "^5.1.4"
    }
}
