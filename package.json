{
  "name": "tictactoe",
  "sideEffects": [
    "*.css"
  ],
  "version": "0.1.0",
  "dependencies": {
    "acosgames": "^1.1.2",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "flatstore": "^1.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "socket.io": "^4.1.2",
    "vm2": "^3.9.3"
  },
  "scripts": {
    "start": "npm run client:dev & npm run server:dev & npx acos",
    "client:dev": "npx webpack --config builds/webpack/client/dev.config.js --watch ./game-client",
    "client:prod": "npx webpack --config builds/webpack/client/prod.config.js",
    "server:dev": "npx webpack --config builds/webpack/server/dev.config.js --watch ./game-server",
    "server:prod": "npx webpack --config builds/webpack/server/prod.config.js",
    "deploy": "npm run client:prod && npm run server:prod && npx acos deploy",
    "kill:win": "taskkill //im node.exe //F"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.26.14",
    "chokidar": "^3.5.1",
    "concurrently": "^6.4.0",
    "core-js": "^3.12.1",
    "css-loader": "^5.2.5",
    "file-loader": "^6.2.0",
    "form-data": "^4.0.0",
    "nodemon": "^2.0.15",
    "sass": "^1.33.0",
    "sass-loader": "^11.1.1",
    "source-map-support": "^0.5.19",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "wait-on": "^5.3.0",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "yargs": "^17.0.1"
  }
}
