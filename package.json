{
  "name": "tictactoe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.17.1",
    "flatstore": "^1.0.2",
    "nodemon": "^2.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "socket.io": "^4.1.2",
    "vm2": "^3.9.3"
  },
  "scripts": {
    "start": "concurrently npm:client-dev npm:server-dev npm:simulator npm:simulator-sync",
    "client-dev": "./node_modules/.bin/webpack --watch ./game-client --config fivesecondgames/webpack/client/dev.config.js",
    "server-dev": "./node_modules/.bin/webpack --watch ./game-server --config fivesecondgames/webpack/server/dev.config.js",
    "simulator": "nodemon --inspect --watch fivesecondgames/simulator/*.js fivesecondgames/simulator/server.js",
    "simulator-sync": "./node_modules/.bin/wait-on http://localhost:3000/ && browser-sync start --no-ghost-mode --ws --port 8080 --proxy localhost:3000 --files=./fivesecondgames/builds/client/client.bundle.js --files ./fivesecondgames/simulator/*.html  --ignore=node_modules"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.26.14",
    "concurrently": "^6.1.0",
    "core-js": "^3.12.1",
    "css-loader": "^5.2.5",
    "sass": "^1.33.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "wait-on": "^5.3.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  }
}
